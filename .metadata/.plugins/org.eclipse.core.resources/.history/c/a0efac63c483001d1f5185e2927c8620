spring:
  application:
    name: COURSE-CLOUD-GATEWAY
    
eureka:
  client:
    service-url:
      default-zone: http://localhost:8761
      
server:
  port: 9000
  
cloud:
    gateway:
      routes:
        - id: productService
          uri: lb://PRODUCT-SERVICE
          predicates:
            - Path=/product-api/**