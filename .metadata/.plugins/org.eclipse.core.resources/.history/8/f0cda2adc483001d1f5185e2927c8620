spring:
  application:
    name: COURSE-CLOUD-GATEWAY
  cloud:
    gateway:
      routes:
        - id: courseServiceCrud
          uri: lb://COURSE-CRUD-SERVICE
          predicates:
            - Path=/course-service/**
        - id: courseServiceCrud
          uri: lb://COURSE-CRUD-SERVICE
          predicates:
            - Path=/course-service/**
    
eureka:
  client:
    service-url:
      default-zone: http://localhost:8761
      
server:
  port: 9000
  
