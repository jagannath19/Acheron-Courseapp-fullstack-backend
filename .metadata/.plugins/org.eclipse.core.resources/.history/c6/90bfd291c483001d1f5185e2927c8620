spring:
  application:
    name: COURSE-CLOUD-GATEWAY
  cloud:
    gateway:
      routes:
        - id: courseServiceCrud
          uri: lb://PRODUCT-SERVICE-CRUD
          predicates:
            - Path=/product-api/**
    
eureka:
  client:
    service-url:
      default-zone: http://localhost:8761
      
server:
  port: 9000
  
