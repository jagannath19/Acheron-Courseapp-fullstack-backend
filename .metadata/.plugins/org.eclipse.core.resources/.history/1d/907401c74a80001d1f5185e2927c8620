package com.acheron.courseapp;

import org.springframework.boot.CommandLineRunner;
import org.springframework.boot.SpringApplication;
import org.springframework.boot.autoconfigure.SpringBootApplication;

@SpringBootApplication
public class CourseappCourseServiceApplication implements CommandLineRunner{

	public static void main(String[] args) {
		SpringApplication.run(CourseappCourseServiceApplication.class, args);
	}

	@Override
	public void run(String... args) throws Exception {
		// TODO Auto-generated method stub
		
		Course record2 = new Course("A01", "java course", "developer", "java developer", Arrays.asList("java","spring"), "radhe", 30, 90, "pending", LocalDate.of(2022, 12, 20), LocalDate.of(2023, 01, 20), Arrays.asList("English","Hindi"), Arrays.asList("English","Hindi"), Arrays.asList("quiz test","coding practice"), 10000, 4.5, "pending", "online");
	      
	      index.saveObject(record2).waitTask();
	}

}
